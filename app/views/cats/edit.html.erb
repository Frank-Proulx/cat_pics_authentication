<% if @cat.user_id == current_user.id %>
<h1>Edit cat</h1>

  <%= form_for @cat do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :breed %>
    <%= f.text_field :breed %>
    <%= f.submit  %>
  <% end %>
<% else %>
  <p>This isn't your cat</p>
<%end%>

<p><%= link_to "Return to cats", cats_path %></p>